@{
    Layout = "BaseLayout.cshtml";
}
@RenderSection("Styles", false)
@section BodyContent{
    <link rel="stylesheet"  type="text/css" href="~/Content/css/estilos-logueado.css">
}
@section NavBar{
    <nav class="navbar navbar-expand-md navbar-dark bg-primary fixed-left">
        <a class="navbar-brand" href="#">Consorcios Infonete</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault"
                aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarsExampleDefault">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Inicio</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="#">Consorcios</a>
                </li>
                <li class="nav-item active">
                    <a class="nav-link" href="#">Contact</a>
                </li>
                <li class="nav-item active cerrar-sesion">
                    <a class="text-white d-block mb-2 text-decoration-none" href="/Home/LogOut">Cerrar sesión                   
                    </a>
                </li>
            </ul>
        </div>
    </nav>
    <div class="w-100 d-flex justify-content-end bg-claro fixed-top z-20 navbar-secundaria">
        <a class="btn text my-2 my-sm-0" href="/Home/LogOut">Cerrar sesion</a>
    </div>
    <div class="container py-3 px-3 bg-white container-post-navbar-secundaria">


}
@RenderBody()

</div>
@section Scripts{@RenderSection("Scripts", false)}
